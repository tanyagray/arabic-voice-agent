# Google OAuth (configured in Supabase dashboard)
# Only needed for mobile app authentication
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Soniox Speech Recognition API
SONIOX_API_KEY=your-soniox-api-key

# Webhook base URL for Soniox callbacks (use ngrok/cloudflare tunnel for local dev)
WEBHOOK_BASE_URL=https://your-domain.com

# Resend API Key (for email authentication)
RESEND_API_KEY=re_your_api_key_here
